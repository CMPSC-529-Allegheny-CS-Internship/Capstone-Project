import 'api_base_helper';
import 'response';



class ClusterRepository {
  // final String _apiKey = apiKey;
ApiBaseHelper _helper = ApiBaseHelper();
Future<List<Cluster>> fetchClusterList() async {
  //make sure to put the apiKey into the get later
    final response = await _helper.get("cluster");
    return cluster_Response.fromJson(response).results;
  }
}