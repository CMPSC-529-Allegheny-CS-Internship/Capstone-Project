import 'api_base_helper.dart';
import 'cluster_node_response.dart';



class NodeRepository {
  final String _apiKey = apiKey;
ApiBaseHelper _helper = ApiBaseHelper();
Future<List<Node>> fetchNodeList() async {
  //make sure to put the apiKey into the get later
    final response = await _helper.get("node");
    return NodeResponse.fromJson(response).results;
  }
}